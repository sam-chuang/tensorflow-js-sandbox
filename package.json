{
  "name": "tensorflow-sandbox",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "start": "npm-run-all --parallel build:esm watch:js:build start:server",
    "build:esm": "snowpack --include \"src/**/*.js\" --dest dist/web_modules",
    "build:js": "babel src/ --out-dir dist",
    "watch:js:build": "babel src/ --out-dir dist --watch",
    "start:server": "es-dev-server index.html --watch --open"
  },
  "dependencies": {
    "babel-plugin-htm": "^3.0.0",
    "htm": "^3.0.3",
    "hyperapp": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "save-dev": "0.0.1-security"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "es-dev-server": "^1.40.1",
    "snowpack": "^1.4.0"
  },
  "keywords": []
}
