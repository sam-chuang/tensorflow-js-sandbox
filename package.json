{
  "name": "tensorflow-sandbox",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "npm-run-all --parallel build:esm build:js:watch start:server",
    "start:server": "es-dev-server index.html --watch --open",
    "build:esm": "snowpack --include \"src/**/*.js\" --dest dist/web_modules --hash --source-map",
    "build:js": "babel src/ --out-dir dist",
    "build:js:watch": "babel src/ --out-dir dist --watch",
    "build:index-page": "node --experimental-modules index.html.js"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^1.5.2",
    "babel-plugin-htm": "^3.0.0",
    "htm": "^3.0.3",
    "hyperapp": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.6",
    "csv-parser": "^2.3.2",
    "es-dev-server": "^1.40.1",
    "fs-extra": "^8.1.0",
    "jest": "^25.1.0",
    "jest-chain": "^1.1.5",
    "jest-extended": "^0.11.5",
    "neat-csv": "^5.2.0",
    "snowpack": "^1.4.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test.config.js"
    ]
  },
  "keywords": []
}
